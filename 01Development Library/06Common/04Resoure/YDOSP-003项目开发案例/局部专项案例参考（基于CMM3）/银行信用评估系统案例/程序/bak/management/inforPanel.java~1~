public class inforPanel extends JPanel {
  BorderLayout borderLayout1 = new BorderLayout();

  public inforPanel() {
    try {
      jbInit();
    }
    catch(Exception ex) {
      ex.printStackTrace();
    }
  }
  void jbInit() throws Exception {
    this.setLayout(borderLayout1);
  }
}