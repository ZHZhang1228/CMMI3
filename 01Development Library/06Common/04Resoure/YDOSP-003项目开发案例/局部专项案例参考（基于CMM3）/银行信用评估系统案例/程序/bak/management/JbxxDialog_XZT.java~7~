  public JbxxDialog_XZT(Frame frame, String title, boolean modal) {
    super(frame, title, modal);
    try {
      jbInit();
      pack();
    }
    catch(Exception ex) {
      ex.printStackTrace();
    }
  }
  public JbxxDialog_XZT(Frame frame, String title, boolean modal,String str1,String str2) {
    super(frame, title, modal);
    username=str1;
    usercard=str2;
   try {
     jbInit();
     pack();
   }
   catch(Exception ex) {
     ex.printStackTrace();
   }
 }



